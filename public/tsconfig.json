    "target": "ES2017",
    "module": "ES2020",
    "moduleResolution": "node",
    "rootDir": "js",
    "outDir": "js",
    "allowJs": true,
    "noEmit": false,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["js/**/*.ts"],
  "exclude": []
}
